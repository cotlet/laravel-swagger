{
  "name": "saeedvaziry/laravel-swagger",
  "description": "Laravel Swagger Generator",
  "license": "MIT",
  "authors": [
    {
      "name": "Saeed Vaziry",
      "email": "sa.vaziry@gmail.com"
    }
  ],
  "require": {
    "php": ">=7.1.3",
    "laravel/framework": "^5.5",
    "zircote/swagger-php": "~2.0|3.*",
    "swagger-api/swagger-ui": "^3.0",
    "symfony/yaml": "^4.1"
  },
  "require-dev": {
    "phpunit/phpunit": "7.*",
    "mockery/mockery": "1.*",
    "orchestra/testbench": "3.6.*",
    "php-coveralls/php-coveralls": "^2.0"
  },
  "suggest": {
    "zircote/swagger-php:~2.0": "!!! Require Swagger-PHP ~2.0 for @SWG annotations support !!!"
  },
  "autoload": {
    "psr-4": {
      "SaeedVaziry\\LaravelSwagger\\": "src"
    },
    "files": [
      "src/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "SaeedVaziry\\LaravelSwagger\\LaravelSwaggerServiceProvider"
      ]
    }
  },
  "prefer-stable": true
}
